// the size function creates a window with a set amount of width and height pixels
size(500, 500);

background(255,255,0);

// the fill function fills the following shape(s) with the color red, green, blue
fill(50,100,120);


stroke(255,0,255);
strokeWeight(10);
rect(100,100,60,60);





// the rect function draws a rectangle at initial coordinates x, -y, and then for size width, height
rect(40,40,60,60);

ellipse(100,100,100,100);

line(100,100,width,0);